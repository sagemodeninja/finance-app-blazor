@page "/"
@using Microsoft.AspNetCore.Authorization
@attribute [Authorize]

<BFUStack Horizontal="true"
          VerticalFill="true"
          Wrap="true"
          Tokens=@(new BFUStackTokens { ChildrenGap = new[] { 20.0 } })
          Style="padding-top:50px;box-sizing:border-box;">
    @for(var i=0; i<5; i++) {
        <BFUStackItem>
            <AccountCard />
        </BFUStackItem>
    }
</BFUStack>